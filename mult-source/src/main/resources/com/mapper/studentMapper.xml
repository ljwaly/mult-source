<!-- 		resultType="com.ljw.dao.entity.Emp"> -->
<!-- 		select * from t_emp where ename=#{ename} -->
		
<!-- 	</select> -->
	<!-- 查询全部员工 -->
	<insert id="save">
		<selectKey resultType="long" keyProperty="studentId" order="AFTER">
			SELECT LAST_INSERT_ID() AS id 
		</selectKey>
		insert into TABLE_STU (NAME,AGE,SEX) values (#{name},#{age},#{sex}) 
	</insert>
</mapper> 